! Include colorscheme from ~/.palettes
! Defines *{back,fore}ground, along with *color[1-15].
! And possibly other things, like cursor colour.
#include ".palettes/solarized/solarized"
! #include "palettes/base16/base16-solarized.dark.xresources"

! TODO: Find way to use MAIN_FONT,POWERLINE_FONT
! - currently it breaks when using both (space is inserted after)
#define MAIN_FONT -*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*
#define BOLD_FONT -*-gohufont-bold-*-*-*-11-*-*-*-*-*-iso10646-*
#define ITALIC_FONT -*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*
#define BOLD_ITATLIC_FONT -*-gohufont-bold-*-*-*-11-*-*-*-*-*-iso10646-*

#define POWERLINE_FONT xft:Inconsolata for Powerline:pixelsize=12

! Compile xft: Turn on/off double-buffering for xft (default enabled).  On some card/driver combination enabling it slightly decreases performance, on most it greatly helps it. The slowdown is small. so it should noramlly be enabled.
URxvt*buffered: true


! True: specify that jump scrolling should be used. When receiving lots of lines, urxvt will only scroll once a whole screen height of lines has been read, resulting in fewer updates
URxvt*jumpScroll: true

! True: (the default) specify that skip scrolling should be used. When receiving lots of lines, urxvt will only scroll once in a while (around 60 times per second), resulting in far
URxvt*skipScroll: false

*font:           -*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*
*boldFont:       -*-gohufont-bold-*-*-*-11-*-*-*-*-*-iso10646-*
*italicFont:     -*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*
*boldItalicFont: -*-gohufont-bold-*-*-*-11-*-*-*-*-*-iso10646-*

!URxvt*font: MAIN_FONT,POWERLINE_FONT
URxvt*font: -*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*,xft:Inconsolata for Powerline:pixelsize=11
!URxvt*font: xft:PowerlineSymbols:pixelsize=11,-*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*


!URxvt*font: -*-gohufont-medium-*-*-*-11-*-*-*-*-*-iso10646-*,xft:Inconsolata\ for\ Powerline:pixelsize=12
