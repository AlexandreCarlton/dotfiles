#!/bin/sh

. ~/.bin/export_colours_bar.sh

COLOR_FOREGROUND=$foreground
COLOR_BACKGROUND=$background
# Currently selected workspace with something in it
COLOR_FOCUSED_OCCUPIED_FG=$white
COLOR_FOCUSED_OCCUPIED_BG=$yellow
# Currently selected workspace with nothing in it.
COLOR_FOCUSED_FREE_FG=$foreground
COLOR_FOCUSED_FREE_BG=$background
# Not sure what 'urgent' means here.
COLOR_FOCUSED_URGENT_FG=$foreground
COLOR_FOCUSED_URGENT_BG=$background
# Workspace with something in it.
COLOR_OCCUPIED_FG=$white
COLOR_OCCUPIED_BG=$background 
# Workspace with nothing in it.
COLOR_FREE_FG=$foreground
COLOR_FREE_BG=$background
COLOR_URGENT_FG=$foreground
COLOR_URGENT_BG=$background
COLOR_LAYOUT_FG=$foreground
COLOR_LAYOUT_BG=$background
COLOR_TITLE_FG=$foreground
COLOR_TITLE_BG=$background
COLOR_STATUS_FG=$foreground
COLOR_STATUS_BG=$background

